{
	"name": "vba-blocks",
	"version": "0.5.3",
	"repository": "https://github.com/vba-blocks/vba-blocks.git",
	"author": "Tim Hall <tim.hall.engr@gmail.com> (https://github.com/timhall)",
	"license": "MIT",
	"bin": "lib/vba-blocks.js",
	"main": "lib/index.js",
	"scripts": {
		"format": "prettier --write \"**/*.{ts,js}\"",
		"test": "jest --runInBand",
		"typecheck": "tsc",
		"test:e2e": "jest --config e2e.config.js --runInBand",
		"build": "rimraf lib && rollup -c && node scripts/ensure-vendor",
		"build:dev": "cross-env NODE_ENV=development rollup -c",
		"build:addins": "cd addins && node --no-warnings ../scripts/build-addins",
		"build:bootstrap": "cd scripts/bootstrap && node --no-warnings ../../lib/vba-blocks build",
		"version": "npm run build && npm run build:addins && node scripts/ensure-vendor && node scripts/create-packages",
		"postversion": "git push && git push --tags",
		"release": "node scripts/release",
		"clean": "rimraf lib && rimraf dist && rimraf addins/build"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@timhall/ansi-colors": "^5.0.0",
		"@timhall/dedent": "^0.9.1",
		"archiver": "^3",
		"conf": "^3",
		"date-fns": "^2",
		"debug": "^4",
		"decompress": "^4",
		"editorconfig": "^0.15.3",
		"env-paths": "^2",
		"fs-extra": "^7",
		"isomorphic-git": "^0.58",
		"logic-solver": "^2",
		"meant": "^1.0",
		"mri": "^1",
		"node-fetch": "^3",
		"open": "^6",
		"pretty-hrtime": "^1",
		"sanitize-filename": "^1",
		"semver": "^5",
		"tmp": "^0.0",
		"toml-patch": "^0.2",
		"walk-sync": "^1",
		"xml-js": "^1.6"
	},
	"devDependencies": {
		"@octokit/core": "^3.1.4",
		"@octokit/rest": "^16.21.1",
		"@rollup/plugin-commonjs": "^15.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-typescript": "^6.0.0",
		"@types/archiver": "^2",
		"@types/fs-extra": "^5",
		"@types/jest": "^23",
		"@types/node": "^10",
		"@types/node-fetch": "^2.1.7",
		"@types/semver": "^5",
		"@types/tmp": "^0.0",
		"aws-sdk": "^2.562.0",
		"builtin-modules": "^3.0.0",
		"cross-env": "^5",
		"dotenv": "^7.0.0",
		"editor": "^1.0.0",
		"jest": "^26.6.1",
		"mustache": "^3",
		"npm-run-all": "^4",
		"prettier": "^1",
		"rimraf": "^2",
		"rollup": "^2.32.1",
		"rollup-plugin-terser": "^7.0.2",
		"ts-jest": "^26.4.2",
		"tslib": "^1.9.3",
		"typescript": "^3",
		"url-join": "^4.0.1"
	}
}
